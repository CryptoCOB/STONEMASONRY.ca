# Database connection string
DATABASE_URL=postgres://postgres:postgres@localhost:5432/simcoe_platform

# JWT secret for issuing tokens
JWT_SECRET=change-me

# Public API key for unauthenticated intake flows (quote requests, reviews)
PUBLIC_API_KEY=public-intake-key

# Default account to attribute public submissions to
DEFAULT_ACCOUNT_ID=00000000-0000-0000-0000-000000000000

# Optional server port override
PORT=3001
